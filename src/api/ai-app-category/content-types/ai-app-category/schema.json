{
  "kind": "collectionType",
  "collectionName": "ai_app_categories",
  "info": {
    "singularName": "ai-app-category",
    "pluralName": "ai-app-categories",
    "displayName": "AI APP Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "ai_apps": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ai-app.ai-app",
      "mappedBy": "category"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "file-to-text",
        "voice-to-text"
      ]
    },
    "image": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    }
  }
}
