{
  "kind": "collectionType",
  "collectionName": "ai_app_apis",
  "info": {
    "singularName": "ai-app-api",
    "pluralName": "ai-app-apis",
    "displayName": "AI App API"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "endpoint": {
      "type": "string",
      "required": true
    },
    "method": {
      "type": "enumeration",
      "enum": [
        "POST",
        "GET"
      ],
      "default": "POST"
    },
    "form_data": {
      "type": "json"
    },
    "bearer_token": {
      "type": "text"
    },
    "short_desc": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "file-to-text",
        "audio-to-text",
        "text-to-audio"
      ],
      "required": true
    },
    "ai_app": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::ai-app.ai-app",
      "mappedBy": "api"
    }
  }
}
