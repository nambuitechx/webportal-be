{
  "kind": "collectionType",
  "collectionName": "ai_apps",
  "info": {
    "singularName": "ai-app",
    "pluralName": "ai-apps",
    "displayName": "AI App"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "short_desc": {
      "type": "text"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "ai_app_status": {
      "type": "enumeration",
      "enum": [
        "pilot",
        "development",
        "production"
      ],
      "required": true
    },
    "ai_app_api": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::ai-app-api.ai-app-api",
      "inversedBy": "ai_app"
    }
  }
}
